{
  "multipart": [
    {
      "when": {
        "bottom": "false",
		"waterlogged": "false"
      },
      "apply": {
        "model": "minecraft:block/scaffolding_stable"
      }
    },
	{
      "when": {
        "bottom": "false",
		"waterlogged": "true"
      },
      "apply": {
        "model": "minecraft:block/waterlogged_scaffolding_stable"
      }
    },
	
	{
      "when": {
        "bottom": "true",
		"waterlogged": "false"
      },
      "apply": {
        "model": "minecraft:block/scaffolding_unstable"
      }
    },
	{
      "when": {
        "bottom": "true",
		"waterlogged": "true"
      },
      "apply": {
        "model": "minecraft:block/waterlogged_scaffolding_unstable"
      }
    }
  ]
}